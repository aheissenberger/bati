{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "inputs": ["$TURBO_DEFAULT$", "files/**", "hooks/**"],
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "env": ["BUNDLE"]
    },
    "test": {
      "dependsOn": ["build"]
    },
    "check-types": {}
  },
  "daemon": false
}
